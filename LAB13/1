//Дано вещественное число — цена 1 кг конфет. Вывести стоимость 0.1, 0.2, . . . , 1 кг конфет.
#include <stdio.h>
    int main()
    {
        system("chcp 1251");
        float n;
        printf("Введите цену за кг  ");
        scanf_s("%f", &n);
        printf(" цена за 0,1 кг %f ",( 0.1*n) );
        printf(" цена за 0,2 кг %f ", (0.2 * n));
        printf(" цена за 0,3 кг %f ", (0.3 * n));
        printf(" цена за 0,4 кг %f ", (0.4 * n));
        printf(" цена за 0,5 кг %f ", (0.5 * n));
        printf(" цена за 0,6 кг %f ", (0.6 * n));
        printf(" цена за 0,7 кг %f ", (0.7 * n));
        printf(" цена за 0,8 кг %f ", (0.8 * n));
        printf(" цена за 0,9 кг %f ", (0.9 * n));
        return 0;
        }
