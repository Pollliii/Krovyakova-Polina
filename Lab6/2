#include <math.h> // подключаемем библиотеку для использования математических операций
#include <stdio.h> // подключаем библиотеку ввод/вывода на экран
int main() //определяем функцию
{
	
	int  a,b,c,m,n;// вводим переменные без значени 
	printf("Enter the a ");//просим пользователя ввести a
	scanf_s ("%d", &a);//ввод
	printf("Enter the b ");//просим пользователя ввести b
scanf_s("%d", &b);//ввод
printf("Enter the c ");//просим пользователя ввести c
scanf_s("%d", &c);//ввод
n = b;
b = a;
a = c;
c = n ;

printf("result a- %d,b- %d, c- %d",a,b,c);
	return 0;
}
