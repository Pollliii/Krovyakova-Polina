//Дано целое положительное число. Проверить истинность высказывания: «Данное число является четным двузначным».
#include <math.h> // подключаемем библиотеку для использования математических операций
#include <stdio.h> // подключаем библиотеку ввод/вывода на экран
int main() //определяем функцию
{

	int  a,b,c;// вводим переменные без значени 
	printf("Enter the a ");//просим пользователя ввести 
	scanf_s("%d", &a);//ввод
	b = a % 2;
	if ((b==0) && (a<100)&& (a > 9))
		printf("true ");
	else printf("false ");
	return 0;
}
