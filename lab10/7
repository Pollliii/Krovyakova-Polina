//Даны целые числа a, b, c. Проверить истинность высказывания: «Существует треугольник со сторонами a, b, c».
#include <math.h> // подключаемем библиотеку для использования математических операций
#include <stdio.h> // подключаем библиотеку ввод/вывода на экран
int main() //определяем функцию
{

	int  a,b,c;// вводим переменные без значени 
	printf("Enter  a ");//просим пользователя ввести сторону
	scanf_s("%d", &a);//ввод
	printf("Enter  b ");//просим пользователя ввести сторону
	scanf_s("%d", &b);//ввод
	printf("Enter  c ");//просим пользователя ввести сторону
	scanf_s("%d", &c);//ввод
	
	if ((a+b>c)&&(a+c > b)&&(c + b > a))
		printf("exist");
	else
		printf("not exist");
	return 0;
}
