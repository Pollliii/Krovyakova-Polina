//Даны три целых числа: A, B, C. Проверить истинность высказывания: «Справедливо двойное неравенство A < B < C»
#include <math.h> // подключаемем библиотеку для использования математических операций
#include <stdio.h> // подключаем библиотеку ввод/вывода на экран
int main() //определяем функцию
{

	int  a,b,c;// вводим переменные без значени 
	printf("Enter the a ");//просим пользователя ввести 
	scanf_s("%d", &a);//ввод
	printf("Enter the b ");//просим пользователя ввести 
	scanf_s("%d", &b);//ввод
	printf("Enter the c ");//просим пользователя ввести  
	scanf_s("%d", &c);//ввод
	if ((a<b) && (b < c))
		printf("true ");
	else printf("false ");
	return 0;
}
